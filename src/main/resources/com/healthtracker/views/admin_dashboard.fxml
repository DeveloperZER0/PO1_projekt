<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.healthtracker.ui.AdminDashboardController">

    <top>
        <Label text="Panel administratora" style="-fx-font-size: 20; -fx-font-weight: bold; -fx-padding: 10;"/>
    </top>

    <center>
        <TableView fx:id="userTable">
            <columns>
                <TableColumn fx:id="idColumn" text="ID"/>
                <TableColumn fx:id="usernameColumn" text="Login"/>
                <TableColumn fx:id="emailColumn" text="E-mail"/>
                <TableColumn fx:id="roleColumn" text="Rola"/>
            </columns>
        </TableView>
    </center>
</BorderPane>
